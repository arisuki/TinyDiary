    <div>By: </div>
    <div><%= entry.userName %></div>
    <div><img src="<%= entry.userAvatar %>" alt=""></div>
 
  </section>

<a href="/entries/<%= entry.id %>/edit"><img src="./../../images/edit.png" alt="edit"></a>
