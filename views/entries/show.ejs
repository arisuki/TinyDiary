<%- include('../partials/header.ejs') %>

<h3>
    <%= entry.title %>
</h3>

<section id="show-page">
    <div><img src="<%= entry.image.contentType %>" alt=""></div>
 
    <div>Date: </div>
    <div><%= entry.date %></div>
    <div>Description: </div>
    <div><%= entry.description %></div>
