<%- include('partials/header') %>

<h1 class="typewriter"><%= title %></h1>
<p>
  > log in to see your diary entries
  <br />
  > guests can only see full gallery
</p>

<p>
  <br>
  <% entries.reverse().slice(0,4).forEach(function(entry) { %>
  <a href="/entries/<%= entry.id %>"><img id="image-thumb" src="<%= entry.image %>" title="<%= entry.title %>" /></a>
  <% }); %>
</p>

<%- include('partials/footer') %>
